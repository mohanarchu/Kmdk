<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"

    android:weightSum="3"
    tools:context=".Members.home.MemberHome">




    <LinearLayout
        android:background="@color/background"
        android:weightSum="2"
        android:orientation="vertical"
        android:layout_weight="2"
        android:layout_width="match_parent"
        android:layout_height="0dp">

        <RelativeLayout
            android:padding="5dp"
            android:layout_weight="1.2"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                 android:layout_alignParentLeft="true"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="18dp"
                android:textStyle="bold"
                android:text="@string/title_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:layout_centerInParent="true"
                android:background="@drawable/logos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:textStyle="bold"
             android:layout_alignParentRight="true"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="18dp"
                android:text="@string/title_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="@color/colorPrimaryDark"
            />
        <LinearLayout
            android:visibility="gone"
            android:orientation="vertical"
            android:layout_marginLeft="4dp"
            android:layout_weight="0.8"
            android:layout_width="match_parent"
            android:layout_height="0dp">


            <TextView
                android:id="@+id/memNewCom"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:textStyle="bold"
                android:text="புதிய புகார்கள்"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <View
                android:layout_width="match_parent"
                android:layout_height="0.01dp"
                android:background="@color/colorPrimary"
                />
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <LinearLayout
                        android:weightSum="2"
                        android:layout_marginTop="3dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_weight="0.4"
                            android:textStyle="bold"
                            android:padding="3dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:text="@string/name"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_weight="1"
                            android:padding="3dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:text=":  mohanrah"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" />
                        <LinearLayout
                            android:layout_marginRight="4dp"
                            android:gravity="end"
                            android:layout_gravity="end"
                            android:layout_weight="0.6"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:layout_gravity="center"
                                android:src="@drawable/clock"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent" />
                            <TextView
                                android:layout_marginRight="5dp"
                                android:padding="3dp"
                                android:textSize="15dp"
                                android:textColor="@color/black"
                                android:text="12.30 PM"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </LinearLayout>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.01dp"
                        android:background="@color/grey"
                        />
                    <LinearLayout
                        android:weightSum="2"
                        android:layout_marginTop="3dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_weight="0.4"
                            android:textStyle="bold"
                            android:padding="3dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:text="@string/place"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_weight="1"
                            android:padding="3dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:text=":  கோயம்புத்தூர்"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.01dp"
                        android:background="@color/grey"
                        />
                    <LinearLayout
                        android:weightSum="2"
                        android:layout_marginTop="3dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_weight="0.4"
                            android:textStyle="bold"
                            android:padding="3dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:text="செய்தி"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_weight="1"
                            android:padding="3dp"
                            android:textSize="16dp"
                            android:textColor="@color/black"
                            android:text=":  கோயம்புத்தூர்"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.01dp"
                        android:background="@color/grey"
                        />
                </LinearLayout>
                <RelativeLayout

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/memSeeAll"
                        android:layout_marginRight="4dp"
                        android:textStyle="bold"
                        android:textColor="@color/text_red"
                        android:textSize="17dp"
                        android:text="புகார்களைப் பார்க்க"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                </RelativeLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:visibility="gone"
            android:weightSum="6"
            android:orientation="vertical"
            android:layout_marginLeft="4dp"
            android:layout_weight="0.8"
            android:layout_width="match_parent"
            android:layout_height="0dp">


            <TextView
                android:id="@+id/memNewMem"
                android:layout_weight="0.9"
                  android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:text="புதிய உறுப்பினர்கள்"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <View
                android:layout_width="match_parent"
                android:layout_height="0.01dp"
                android:background="@color/colorPrimary"
                />
            <LinearLayout
                android:layout_weight="5"
                android:weightSum="5"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="0dp">


                <LinearLayout
                    android:layout_weight="1"
                    android:weightSum="2"

                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="0dp">
                    <TextView
                        android:layout_weight="0.4"
                        android:textStyle="bold"
                        android:padding="3dp"
                        android:textSize="16dp"
                        android:textColor="@color/black"
                        android:text="@string/name"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_weight="0.7"
                        android:padding="3dp"
                        android:textSize="16dp"
                        android:textColor="@color/black"
                        android:text=":  mohanrah"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                    <LinearLayout
                        android:layout_marginRight="4dp"
                        android:gravity="end"
                        android:layout_gravity="end"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top"
                            android:text="3 "
                            android:textSize="17dp"
                            android:textStyle="bold" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top"
                            android:text="@string/febrary"

                            android:textSize="16dp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top"
                            android:text=" , 2016"
                            android:textSize="16dp"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.01dp"
                    android:background="@color/grey"
                    />
                <LinearLayout
                    android:layout_weight="1"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="0dp">
                    <TextView
                        android:layout_weight="0.4"
                        android:textStyle="bold"
                        android:padding="3dp"
                        android:textSize="16dp"
                        android:textColor="@color/black"
                        android:text="@string/place"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_weight="1"
                        android:padding="3dp"
                        android:textSize="16dp"
                        android:textColor="@color/black"
                        android:text=":  கோயம்புத்தூர்"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.01dp"
                    android:background="@color/grey"
                    />
                <LinearLayout
                    android:layout_weight="1"
                    android:weightSum="2"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="0dp">
                    <TextView
                        android:layout_weight="0.4"
                        android:textStyle="bold"
                        android:padding="3dp"
                        android:textSize="16dp"
                        android:textColor="@color/black"
                        android:text="@string/district"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_weight="1"
                        android:padding="3dp"
                        android:textSize="16dp"
                        android:textColor="@color/black"
                        android:text=":  கோயம்புத்தூர்"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"

                    android:layout_marginRight="0dp"
                    android:layout_marginBottom="3dp"
                    android:gravity="center|end"
                    android:orientation="horizontal">

                    <android.support.v7.widget.CardView
                        android:layout_width="80dp"
                        android:layout_height="30dp"
                        app:cardBackgroundColor="@color/colorPrimaryDark"
                        app:cardCornerRadius="8dp">

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_edit" />

                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                        android:layout_width="80dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        app:cardBackgroundColor="@color/red_normal"
                        app:cardCornerRadius="8dp">

                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_delete_black_24dp" />

                    </android.support.v7.widget.CardView>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.01dp"
                    android:background="@color/grey"
                    />

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_weight="0.8"
            android:background="@color/background"
            android:layout_width="match_parent"
            android:padding="3dp"
            android:layout_height="0dp">
            <android.support.v7.widget.CardView
                app:cardCornerRadius="4dp"
                app:cardBackgroundColor="@color/background"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:weightSum="2"
                    android:layout_height="match_parent">
                    <android.support.v7.widget.CardView
                        android:id="@+id/homeAdvertise"
                        android:layout_margin="2dp"
                       app:cardBackgroundColor="@color/background"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent">

                        <LinearLayout

                            android:orientation="vertical"
                            android:weightSum="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <ImageView
                                android:src="@drawable/ic_event"
                                android:tint="@color/colorPrimaryDark"
                                android:layout_weight="0.4"
                                android:layout_gravity="center"
                                android:layout_width="50dp"
                                android:layout_height="50dp" />

                            <TextView
                                android:gravity="center"
                                android:id="@+id/memHomeAds"
                                android:textSize="17dp"
                                android:textColor="@color/black"
                                android:textStyle="bold"
                                android:layout_weight="0.6"
                                android:layout_width="match_parent"
                                android:layout_height="0dp" />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:id="@+id/homeComplaint"
                        android:layout_margin="2dp"
                        app:cardBackgroundColor="@color/background"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent">

                        <LinearLayout

                            android:orientation="vertical"
                            android:weightSum="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <ImageView
                                android:src="@drawable/customer"
                                android:tint="@color/colorPrimaryDark"
                                android:layout_weight="0.4"
                                android:layout_width="50dp"
                                android:layout_gravity="center"
                                android:layout_height="50dp" />

                            <TextView
                                android:text="@string/hello_blank_fragment"
                                android:gravity="center"
                                android:id="@+id/complaintsNew"
                                android:textSize="17dp"
                                android:textColor="@color/black"
                                android:textStyle="bold"
                                android:layout_weight="0.6"
                                android:layout_width="match_parent"
                                android:layout_height="0dp" />


                        </LinearLayout>


                    </android.support.v7.widget.CardView>

                </LinearLayout>




            </android.support.v7.widget.CardView>





        </LinearLayout>




    </LinearLayout>

    <RelativeLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp">

        <LinearLayout

            android:orientation="vertical"
            android:weightSum="3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:weightSum="2"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="0dp">

                <android.support.v7.widget.CardView
                    android:id="@+id/homecontactAdmins"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="8dp"
                    app:cardBackgroundColor="@color/greys"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:backgroundTint="@color/white"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_call_red_500_24dp"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />
                        <TextView
                            android:id="@+id/homeContact"
                            android:layout_marginLeft="10dp"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:textSize="16dp"
                            android:text="@string/gallery"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />



                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/homeDown"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="8dp"
                    app:cardBackgroundColor="@color/greys"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:backgroundTint="@color/white"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_file_download_white_24dp"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />
                        <TextView
                            android:id="@+id/homeDownloads"
                            android:layout_marginLeft="10dp"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:textSize="16dp"
                            android:text="@string/events"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />



                    </LinearLayout>
                </android.support.v7.widget.CardView>

            </LinearLayout>
            <LinearLayout
                android:weightSum="2"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="0dp">

                <android.support.v7.widget.CardView
                    android:id="@+id/memGallary"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="8dp"
                    app:cardBackgroundColor="@color/greys"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:backgroundTint="@color/white"
                             android:layout_gravity="center"
                            android:background="@drawable/ic_gallery"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />
                        <TextView
                           android:id="@+id/memHomeGal"
                            android:layout_marginLeft="10dp"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:textSize="16dp"
                            android:text="@string/gallery"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />



                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/memEvents"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="8dp"
                    app:cardBackgroundColor="@color/greys"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:backgroundTint="@color/white"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_events"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />
                        <TextView
                            android:id="@+id/memHomeEvents"
                            android:layout_marginLeft="10dp"
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:textSize="16dp"
                            android:text="@string/events"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />



                    </LinearLayout>
                </android.support.v7.widget.CardView>

            </LinearLayout>



            <LinearLayout
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="0dp">

                <android.support.v7.widget.CardView
                    android:id="@+id/advertise"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="8dp"
                    app:cardBackgroundColor="@color/greys"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:backgroundTint="@color/white"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_add_circle_outline_red_500_24dp"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />




                            <TextView
                                android:id="@+id/memHomeAd"
                                android:paddingLeft="10dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="center"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:textSize="16dp"
                                android:singleLine="true"
                                android:text="@string/advertisements"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />





                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView

                    android:id="@+id/memMember"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="8dp"
                    app:cardBackgroundColor="@color/greys"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:backgroundTint="@color/white"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_member"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:visibility="gone"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="center"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:textSize="16dp"
                                android:singleLine="true"
                                android:text="புதிய"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />


                            <TextView
                                android:id="@+id/memHomeMem"
                                android:layout_marginLeft="3dp"
                                android:layout_gravity="center"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:textSize="16dp"
                                android:singleLine="true"
                                android:text="உறுப்பினர்"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />



                        </LinearLayout>


                    </LinearLayout>
                </android.support.v7.widget.CardView>

            </LinearLayout>


        </LinearLayout>
    </RelativeLayout>





</LinearLayout>